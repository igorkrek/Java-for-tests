z = [1, 7, 6, 4, 5, 9, 8]
i = len(z)
print(' РґР»РёРЅР° РјР°СЃСЃРёРІР° - ' ,len(z))
b = 0
a = 0
c = 0;
while a != len(z): 
    b = a;
    while  b != len(z):
        if  z[b] > z[a] & b > a: 
            print('РёРЅРІРµСЂСЃРёСЏ ' , z[b] ,z[a])
            c +=1
        b +=1 
    a +=1
print('РІСЃРµРіРѕ ', c)
